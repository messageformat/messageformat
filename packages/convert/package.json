{
  "name": "messageformat-convert",
  "version": "0.4.0",
  "description": "Convert input (e.g. from Rails i18n) into messageformat-compatible JSON",
  "keywords": [
    "messageformat",
    "convert",
    "rails",
    "i18n"
  ],
  "author": "Eemeli Aro <eemeli@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/messageformat/messageformat.git",
    "directory": "packages/convert"
  },
  "main": "index.js",
  "engines": {
    "node": ">=6.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-control-regex": 0
    }
  },
  "dependencies": {
    "make-plural": "^6.0.1"
  }
}
